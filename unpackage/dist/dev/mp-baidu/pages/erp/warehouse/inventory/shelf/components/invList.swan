<view><view style="margin-bottom:50px"><view class="uni-list"><checkbox-group bindchange="{{d}}"><block s-for="value in a trackBy value.a"><view class="uni-list-cell" hover-class="uni-list-cell-hover"><view class="uni-media-list"><label s-if="{{b}}" class="radio" style="padding-top:20px"><checkbox style="transform:scale(0.7)" color="#FF6B00" value="{{value.b}}" checked="{{value.c}}"/></label><image bindtap="{{value.d}}" class="uni-media-list-logo" src="{{value.e}}"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top"><text>{{value.f}}</text><view><text class="light-font">SKU:{{value.g}}</text></view><view><text class="text-orange" style="font-weight:bolder">{{value.h}}</text><text style="padding-left:10px;font-size:10px !important" class="light-font">库存</text><input s-if="{{c}}" type="number" class="pullright" style="border:1px solid #eee;border-radius:3px;width:50px" placeholder="{{value.i}}" value="{{value.j}}" bindinput="{{value.k}}"/></view></view></view></view></view></block></checkbox-group></view><uni-load-more s-if="{{e}}" u-i="0efdaed0-0" u-p="{{e}}"/></view><view s-if="{{f}}" class="uni-container tabbar"><view style="position:fixed;left:10px;bottom:10px;padding:5px"><label class="radio"><checkbox-group bindchange="{{i}}"><checkbox style="transform:scale(0.7)" disabled="{{g}}" color="#FF6B00" value="all" checked="{{h}}"/>全选 </checkbox-group></label></view><view style="position:fixed;left:140px;bottom:15px;padding:5px">已选产品<text class="text-orange">({{j}})</text></view><view style="position:fixed;right:10px;bottom:10px;padding:8px;background:#FF6B00;color:#fff"><text bindtap="{{k}}">提交下架</text></view></view></view>